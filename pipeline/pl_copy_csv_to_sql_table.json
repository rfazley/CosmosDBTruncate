									"name": "distinguishing_features",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "Geographical Region",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "geographical_region",
									"type": "String",
									"physicalType": "varchar"
